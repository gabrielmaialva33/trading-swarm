# Trading Swarm Environment Variables Example
# Copy this file to .env and fill in your actual API credentials

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/trading_swarm_dev
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Phoenix Configuration
SECRET_KEY_BASE=generate-with-mix-phx-gen-secret
PHX_HOST=localhost
PORT=4000

# AI APIs
NVIDIA_API_KEY=your-nvidia-api-key-here
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Trading Brokers APIs

# Kraken (Crypto Exchange)
KRAKEN_API_KEY=your-kraken-api-key-here
KRAKEN_API_SECRET=your-kraken-api-secret-here

# Markets4you (Forex)
MARKETS4YOU_API_KEY=your-markets4you-api-key-here
MARKETS4YOU_API_SECRET=your-markets4you-api-secret-here
MARKETS4YOU_BASE_URL=https://api.markets4you.com/v1

# Binary Options
POCKET_OPTION_API_KEY=your-pocket-option-api-key-here
POCKET_OPTION_API_SECRET=your-pocket-option-api-secret-here

OLYMP_TRADE_API_KEY=your-olymp-trade-api-key-here
OLYMP_TRADE_API_SECRET=your-olymp-trade-api-secret-here

IQ_OPTION_API_KEY=your-iq-option-api-key-here
IQ_OPTION_API_SECRET=your-iq-option-api-secret-here

QUOTEX_API_KEY=your-quotex-api-key-here
QUOTEX_API_SECRET=your-quotex-api-secret-here

# Forex Brokers
TIO_MARKETS_API_KEY=your-tio-markets-api-key-here
TIO_MARKETS_API_SECRET=your-tio-markets-api-secret-here

ROBO_FOREX_API_KEY=your-robo-forex-api-key-here
ROBO_FOREX_API_SECRET=your-robo-forex-api-secret-here

CAPITAL_COM_API_KEY=your-capital-com-api-key-here
CAPITAL_COM_API_SECRET=your-capital-com-api-secret-here

EXNESS_API_KEY=your-exness-api-key-here
EXNESS_API_SECRET=your-exness-api-secret-here

# Risk Management
MAX_DAILY_LOSS_PERCENT=5.0
MAX_POSITION_SIZE_PERCENT=2.0
CORRELATION_THRESHOLD=0.7

# Logging
LOG_LEVEL=info
ENABLE_DEBUG=false